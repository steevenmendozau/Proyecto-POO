<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23" fx:controller="Controladores.controladorEstudiantes">
    <children>
      <Pane prefHeight="467.0" prefWidth="730.0">
         <children>
              <TableView fx:id="tablaEstudiantes" layoutX="-1.0" prefHeight="412.0" prefWidth="733.0">
                  <columns>
                      <TableColumn fx:id="colIdEstudiante" prefWidth="100.0" text="ID" />
                      <TableColumn fx:id="colNombreEstudiante" prefWidth="150.0" text="Nombre" />
                      <TableColumn fx:id="colEdadEstudiante" prefWidth="75.0" text="Edad" />
                      <TableColumn fx:id="colMatriculaEstudiante" prefWidth="150.0" text="Matricula" />
                  </columns>
              </TableView>
            <Button layoutX="32.0" layoutY="425.0" mnemonicParsing="false" text="Agregar Estudiante" onAction="#mostrarVistaAgregar"/>
            <Button layoutX="193.0" layoutY="426.0" mnemonicParsing="false" text="Eliminar Estudiante" onAction="#mostrarVistaEliminar"/>
         </children>
      </Pane>
    </children>
</AnchorPane>